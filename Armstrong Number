class Solution {
    public boolean isArmstrong(int n) {
        int sum=0,temp=n;
        while(n>0)
        {
            int ld=n%10;
            n=n/10;
            sum+=(ld*ld*ld);
        }
        if(sum==temp)
        {
            return true;
        }
        else
        {
            return false;
        }


    }
}
