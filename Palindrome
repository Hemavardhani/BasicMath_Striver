class Solution {
    public boolean isPalindrome(int n) {
        int temp=n,rev=0;
        while(n>0)
        {
            int ld=n%10;
            n=n/10;
            rev=(rev*10)+ld;
        }
        if(rev==temp)
        {
            return true;
        }
        else
        {
            return false;
        }

    }
}
